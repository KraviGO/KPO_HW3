{
  "ReverseProxy": {
    "Routes": {
      "storage": {
        "ClusterId": "file-storage",
        "Match": {
          "Path": "/storage/{**catch-all}"
        }
      },
      "analysis": {
        "ClusterId": "file-analysis",
        "Match": {
          "Path": "/analysis/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "file-storage": {
        "Destinations": {
          "storage": {
            "Address": "http://file-storage-service:8080/"
          }
        }
      },
      "file-analysis": {
        "Destinations": {
          "analysis": {
            "Address": "http://file-analysis-service:8080/"
          }
        }
      }
    }
  }
}